  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.just-eat.dk/restaurants*"],
      "js": ["konva.min.js","index.js"]
    }
  ]

}
